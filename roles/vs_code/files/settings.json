        "cava",
        "charmbracelet",
        "chdir",
        "combin",
        "cronie",
        "dbus",
        "dconf",
        "devel",
        "dotfiles",
        "dunst",
        "dunstrc",
        "flathub",
        "flatpak",
        "flatpaks",
        "genericity",
        "goarch",
        "goreleaser",
        "gpgcheck",
        "gpgkey",
        "gresource",
        "hashicorp",
        "homebase",
        "idempotency",
        "incpatch",
        "interwebs",
        "ints",
        "isdir",
        "itertools",
        "katas",
        "libvirtd",
        "lineinfile",
        "lipgloss",
        "lrstanley",
        "metalink",
        "mysecretsalt",
        "neovim",
        "nonfree",
        "nvim",
        "obsproject",
        "papirus",
        "pixmaps",
        "preservim",
        "prettybox",
        "ripgrep",
        "rpmfusion",
        "spicetify",
        "stretchr",
        "struct",
        "termenv",
        "testdata",
        "textinput",
        "titlebar",
        "ulauncher",
        "virtnetworkd",
        "wmctrl",
        "wordvalue",
        "yumrepos",
    ],
    "catppuccin.colorOverrides": {
        "mocha": {
            "green": "#72b3ab"
        }
    },
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "all",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "Vagrantfile": "ruby",
        "gum_colors": "shellscript",
        "**/roles/**/*.yml": "ansible"
    },
    "files.eol": "\n",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git.inputValidation": "always",
    "glassit.alpha": 245,
    "go.inferGopath": true,
    "go.toolsManagement.autoUpdate": true,
    "terminal.integrated.fontFamily": "'CaskaydiaCove Nerd Font Mono'",
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "none",
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ]
}